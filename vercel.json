{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js" 
      },
      {
        "src": "/.well-known/apple-developer-merchantid-domain-association",
        "dest": "/.well-known/apple-developer-merchantid-domain-association"
      }
    ]
  }